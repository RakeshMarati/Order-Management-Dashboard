{
  "name": "karthikeya-boutique-frontend",
  "version": "1.0.0",
  "description": "Frontend for Karthikeya Boutique Order Management System",
  "private": true,
  "dependencies": {
    "axios": "^1.7.9",
    "react": "^18.0.0",
    "react-calendar": "^3.9.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^7.1.5",
    "react-scripts": "5.0.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.12.0",
    "date-fns": "^2.30.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.0",
    "prettier": "^2.8.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "analyze": "npm run build && npx serve -s build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RakeshMarati/Order-Management-Dashboard.git"
  },
  "keywords": [
    "react",
    "boutique",
    "order-management",
    "mern-stack",
    "fashion"
  ],
  "author": "Karthikeya Boutique",
  "license": "MIT"
}
